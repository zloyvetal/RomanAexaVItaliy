<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <!--  tree view definition  -->
        <record id="omo_logs_view_tree" model="ir.ui.view">
            <field name="name">omo_logs.view_tree</field>
            <field name="model">omo.logs</field>
            <field name="arch" type="xml">
                <tree string="Omo logs">
                    <field name="company_id"/>
                    <field name="correlationid"/>
                    <field name="log_severity"/>
                    <field name="timestamp"/>
                    <field name="msg"/>
                    <field name="device_id"/>
                    <field name="device_type"/>
                    <field name="product_id"/>
                    <field name="hub_id"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>